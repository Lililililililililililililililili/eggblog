<!-- app/view/list.nj -->
<html>
  <head>
    <title>eggblog</title>
  </head>
  <body>
    <div class="news-view view">
        <div class="item">
          <h1>hello eggblog</h1>
          <br>
          <h2>{{ id }}</h2>
          <br>
          <h2>{{ name }}</h2>
          <br>
        </div>
        {% for item in list %}
        <div><a href="{{ item.url }}">{{ item.title}}</a><span>{{ helper.relativeTime(1616841540) }}</span></div>
        {% endfor %}
    </div>
  </body>
</html>